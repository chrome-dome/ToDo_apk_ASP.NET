@model Issue

<h1>Create</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Title"></label>
        <input asp-for="Title" />
        <span asp-validation-for="Title"></span>
    </div>
    <div class="form-group">
        <label asp-for="Notes"></label>
        <input asp-for="Notes" />
        <span asp-validation-for="Notes"></span>
    </div>
    <div class="form-group">
        <label asp-for="State"></label>
        <input asp-for="State" />
        <span asp-validation-for="State"></span>
    </div>
    <div class="form-group">
        <label asp-for="IsUrgent"></label>
        <input asp-for="IsUrgent" />
        <span asp-validation-for="IsUrgent"></span>
    </div>
    <div class="form-group">
        <label asp-for="Deadline"></label>
        <input asp-for="Deadline" />
        <span asp-validation-for="Deadline"></span>
    </div>
    <div class="form-group">
        <label asp-for="AssignedTo"></label>
        <input asp-for="AssignedTo" />
        <span asp-validation-for="AssignedTo"></span>
    </div>
</form>
